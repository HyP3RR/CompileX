label f:
param a
param b
	func_prologue  f
		return 0
	func_epilogue  f
label main:
	func_prologue  main
		x = 2
		y = 1
		if x == 1 goto @L0
		if x == 2 goto @L1
		goto @L6
	label @L0:
		param "Outer case 1\n"
		$0 = call printf, 1
		goto @L7
	label @L1:
		param "Outer case 2\n"
		$1 = call printf, 1
		if y == 0 goto @L2
		if y == 1 goto @L3
		goto @L4
	label @L2:
		param "  Inner case 0\n"
		$2 = call printf, 1
		goto @L5
	label @L3:
		param "  Inner case 1\n"
		$3 = call printf, 1
		goto @L5
	label @L4:
		param "  Inner default\n"
		$4 = call printf, 1
	label @L5:
		goto @L7
	label @L6:
		param "Outer default\n"
		$5 = call printf, 1
	label @L7:
		return 0
	func_epilogue  main
